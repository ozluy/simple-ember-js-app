<div class="row">
 {{#each  model as |product| }}
 <div class="col-sm-6 col-md-3">
    <div class="thumbnail text-center">
      <img style="width:150px; height:auto" src="{{product.imageUrl}}" alt="{{product.title}}">
      <div class="caption">
        <h3> {{product.title}}</h3>
        <p>{{product.description}}</p>
        <p><span class="btn btn-lg btn-primary btn-block"> ${{product.price}}</span></p>
      </div>
    </div>
  </div>

 {{/each}}
</div>

